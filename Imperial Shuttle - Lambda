Array::mult = (m) -> @map (v) -> v*m
Array::add = (m) -> @map (v) -> v+m
for key, value of []
  delete Array::[key]
  Object.defineProperty Array::, key,
    value: value
    configurable: true
    enumerable: false
addMainStripeEnd = (n, pos) ->
  model.bodies['bottomConnectStripe'+n] =
      section_segments: [45,135,225,315]
      offset:
        x: 0
        y: -4
        z: pos+0.4
      position:
        x: [0,0,0,0]
        y: [-2,-2,3,3]
        z: [0,0,0,0]
      width: [0,10,10,0]
      height: [0,0.4,0.4,0]
      propeller: false
      texture: [3]
      vertical: true


