elementamount=250

minxpos = 0
maxxpos = 16
minSize = 0.2
maxSize = 0.8
minzpos = -2.5
maxzpos = 2.5
minSize2 = 0.3
maxSize2 = 2.5
sizeArray = []
sizeArray2 = []
xposArray = []
zposArray = []
k=0
loop
  xposArray[k] = Math.random() * (maxxpos - minxpos) + minxpos
  sizeArray[k] = Math.random() * (maxSize - minSize) + minSize
  sizeArray2[k] = Math.random() * (maxSize2 - minSize2) + minSize2
  zposArray[k] = Math.random() * (maxzpos - minzpos) + minzpos
  k = k+1
  if k+1 == elementamount then break


textureArray = [1,2,3,16]
minypos = 0
maxypos = textureArray.length - 1
resultArray = []
l=0
loop
  arrayIndex = Math.round(Math.random() * (maxypos - minypos) + minypos)
  resultArray[l] = textureArray[arrayIndex]
  l = l+1
  if l+1 == elementamount then break

textureArray2 = [1,2,3,4,16,17,63]
minypos = 0
maxypos = textureArray2.length - 1
resultArray2 = []
l=0
loop
  arrayIndex2 = Math.round(Math.random() * (maxypos - minypos) + minypos)
  resultArray2[l] = textureArray2[arrayIndex2]
  l = l+1
  if l+1 == elementamount then break

textureArray3 = [1,4,3,12,3,1,8,2,10]
minypos = 0
maxypos = textureArray3.length - 1
resultArray3 = []
l=0
loop
  arrayIndex3 = Math.round(Math.random() * (maxypos - minypos) + minypos)
  resultArray3[l] = textureArray3[arrayIndex3]
  l = l+1
  if l+1 == elementamount then break




