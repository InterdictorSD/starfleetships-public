Array::mult = (m) -> @map (v) -> v*m
Array::add = (m) -> @map (v) -> v+m
for key, value of []
  delete Array::[key]
  Object.defineProperty Array::, key,
    value: value
    configurable: true
    enumerable: false
addPropulsor = (n,xpos,ypos,zpos,radius,length) ->
  model.bodies['propulsor'+n] =
      section_segments: 16
      offset:
        x: xpos
        y: ypos+mainypos
        z: zpos+mainzpos
      position:
        x: [0,0,0,0,0,0,0]
        y: [0,2,14,80,80,74,74].mult(length)
        z: [0,0,0,0,0,0,0]
      width: [0,5,10,10,7,7,0].mult(radius)
      height: [0,5,10,10,7,7,0].mult(radius)
      texture: [11,3,4,63,4,17]
      propeller: true
addRamStripe = (n, ypos, zpos) ->
  model.bodies['ramStripeMain'+n] =
      section_segments: [45,90+45,180+45,270+45]
      offset:
        x: 0
        y: -294+mainypos+ypos
        z: zpos+mainzpos
      position:
