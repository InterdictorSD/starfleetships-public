        y: [-10,-10,13,13].mult(1.34)
        z: [0,0,0,0]
      width: [0,8,17,0].mult(1.34)
      height: [0,4,4,0].mult(1.34)
      texture: [15.001]
  model.bodies['ramStripeInnerTop'+n] =
      section_segments: [45,90+45,180+45,270+45]
      offset:
        x: 0
        y: -294.02+mainypos+ypos
        z: zpos+2+mainzpos
